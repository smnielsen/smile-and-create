{
  "name": "<%= name %>", 
  "version": "1.0.0",
  "description": "<%= description %>", 
  "main": "build/server/server.js",
  "scripts": {
    "start": "DEBUG=smn* NODE_ENV=production node build/server/server.js",
    "dev": "DEBUG=smn* nodemon -w server ./server/index.js",
    "build": "yarn build:js; yarn build:client & yarn build:server",
    "build:js": "cross-env NODE_ENV=production webpack",
    "build:client": "npx babel client --out-dir build/client",
    "build:server": "npx babel server --out-dir build/server --ignore server/index.js",
    "lint": "eslint ./client/**/**.js ./server/**/**.js ./lib/**/**.js",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage"
  },
  "keywords": [
    "Nodejs",
    "JavaScript",
    "Holidays",
    "Vacations"
  ],
  "author": "<%= author %>",
  "license": "ISC",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/tests/jest.config"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
